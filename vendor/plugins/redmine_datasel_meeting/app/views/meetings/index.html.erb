<h2>Meetings#index</h2>
<table>
	<tr>
		<th>Date</th>
		<th>Subject</th>
	</tr>
<% @meetings.each do |meeting| %>
	<tr>
		<td><%= meeting.date %></td>
		<td><%= link_to meeting.subject, { :action => 'show', :meeting_id => meeting.id, :project_id => @project.identifier} %></td>
	</tr>
<% end %>
</table>
<%= link_to 'New Meeting', { :action => 'new', :project_id => @project.identifier} %>