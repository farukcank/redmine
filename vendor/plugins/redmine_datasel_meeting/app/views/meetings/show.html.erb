<h2>Meetings#show</h2>
<table>
<tr><th>Subject</th><td><%= @meeting.subject %></td></th>
<tr><th>Place</th><td><%= @meeting.place %></td></th>
<tr><th>Date</th><td><%= @meeting.date %></td></th>
<tr><th>Convacator</th><td><%= @meeting.convacator.name %></td></th>
<tr><th colspan='2'>Agenda</th></tr>
<tr>
	<td colspan='2'>
		<div class="wiki">
			<%= textilizable @meeting, :agenda %>
		</div>
	</td>
</tr>
<tr><th colspan='2'>Content</th></tr>
<tr>
	<td colspan='2'>
		<div class="wiki">
			<%= textilizable @meeting, :content %>
		</div>
	</td>
</tr>
</table>
<td><%= link_to 'Update', { :action => 'update', :meeting_id => @meeting.id, :project_id => @project.identifier} %></td>